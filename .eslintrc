{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "indent": 0,
    "linebreak-style": ["error", "unix"],
    "quotes": 0,
    "semi": 0,
    "no-console": [
      "error",
      {
        "allow": ["debug", "info", "warn", "error"]
      }
    ]
  }
}
